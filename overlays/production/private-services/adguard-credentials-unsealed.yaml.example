# Seal with: kubeseal --cert sealed-secrets-pub-cert.pem --format=yaml < overlays/production/private-services/adguard-credentials-unsealed.yaml > overlays/production/private-services/adguard-credentials-sealed.yaml
---
apiVersion: v1
kind: Secret
metadata:
  name: adguard-credentials
  namespace: private-services
type: Opaque
stringData:
  username: "admin"  # Replace with your AdGuard username
  password: "your-password-here"  # Replace with your AdGuard password
