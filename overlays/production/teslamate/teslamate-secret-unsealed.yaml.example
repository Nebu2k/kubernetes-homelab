# Seal with: kubeseal --cert sealed-secrets-pub-cert.pem --format=yaml < overlays/production/teslamate/teslamate-secret-unsealed.yaml > overlays/production/teslamate/teslamate-secret-sealed.yaml
---
apiVersion: v1
kind: Secret
metadata:
  name: teslamate-secrets
  namespace: teslamate
type: Opaque
stringData:
  ENCRYPTION_KEY: "your-secure-encryption-key-here"
  DATABASE_USER: "teslamate"
  DATABASE_PASS: "your-secure-database-password"
  DATABASE_NAME: "teslamate"
