# Seal with: kubeseal --format=yaml --controller-namespace=kube-system < adguard-credentials-unsealed.yaml > adguard-credentials-sealed.yaml
---
apiVersion: v1
kind: Secret
metadata:
  name: homepage-adguard
  namespace: homepage
type: Opaque
stringData:
  username: "admin"  # Replace with your AdGuard username
  password: "your-password-here"  # Replace with your AdGuard password
