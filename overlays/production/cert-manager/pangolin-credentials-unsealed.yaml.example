# Seal with: kubeseal --format=yaml --controller-namespace=kube-system < pangolin-credentials-unsealed.yaml > pangolin-credentials-sealed.yaml
---
apiVersion: v1
kind: Secret
metadata:
  name: pangolin-api-credentials
  namespace: cert-manager
type: Opaque
stringData:
  api-token: "your-pangolin-api-token-here"
  org-id: "your-organization-id-here"
