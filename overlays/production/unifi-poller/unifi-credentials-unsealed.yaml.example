# Seal with: kubeseal --format=yaml --controller-namespace=kube-system < unifi-credentials-unsealed.yaml > unifi-credentials-sealed.yaml
apiVersion: v1
kind: Secret
metadata:
  name: unifi-poller-credentials
  namespace: monitoring
type: Opaque
stringData:
  # UniFi Controller read-only user credentials
  username: "your-unifi-username"
  password: "your-unifi-password"
