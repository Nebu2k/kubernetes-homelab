# kubeseal --cert sealed-secrets-pub-cert.pem --format=yaml < manifests/cert-manager/pangolin-api-credentials-unsealed.yaml > manifests/cert-manager/pangolin-api-credentials-sealed.yaml
---
apiVersion: v1
kind: Secret
metadata:
  name: pangolin-api-credentials
  namespace: cert-manager
type: Opaque
stringData:
  API_KEY: "YOUR_PANGOLIN_API_KEY"
  ORG_ID: "YOUR_ORG_ID"
  SITE_ID: "YOUR_SITE_ID"
  API_BASE_URL: "https://pangolin-api.example.com/v1"
  DOMAIN_ID: "YOUR_DOMAIN_ID"
  DOMAIN_SUFFIX: "example.com"