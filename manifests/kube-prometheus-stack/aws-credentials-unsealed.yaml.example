#kubeseal --cert sealed-secrets-pub-cert.pem --format=yaml < manifests/kube-prometheus-stack/aws-credentials-unsealed.yaml > manifests/kube-prometheus-stack/aws-credentials-sealed.yaml
---
apiVersion: v1
kind: Secret
metadata:
  name: alertmanager-aws-credentials
  namespace: monitoring
type: Opaque
stringData:
  AWS_ACCESS_KEY_ID: "YOUR_AWS_ACCESS_KEY_ID_HERE"
  AWS_SECRET_ACCESS_KEY: "YOUR_AWS_SECRET_ACCESS_KEY_HERE"
  AWS_REGION: "eu-central-1"  # Change to your AWS region
